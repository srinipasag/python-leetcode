from collections import OrderedDict
class Solution(object):
    def firstUniqChar(self, s):
        """
        :type s: str
        :rtype: int
        """
        d = OrderedDict()
        count = 0
        if len(s) is 0:
            return -1
        for i in range(len(s)):
            if s[i] not in d:
                d[s[i]] = i
            else:
                d[s[i]] = False
                
        for key in d:
            if d[key] is not False:
                return d[key]
        return -1
            
